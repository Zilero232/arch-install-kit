# Change YOUR_USERNAME to your username and YOUR_VPN_INTERFACE to your VPN interface.
YOUR_USERNAME ALL=(ALL) NOPASSWD: /usr/bin/wg-quick up YOUR_VPN_INTERFACE
YOUR_USERNAME ALL=(ALL) NOPASSWD: /usr/bin/wg-quick down YOUR_VPN_INTERFACE
YOUR_USERNAME ALL=(ALL) NOPASSWD: /usr/bin/wg-quick status YOUR_VPN_INTERFACE
YOUR_USERNAME ALL=(ALL) NOPASSWD: /usr/bin/wg show YOUR_VPN_INTERFACE
