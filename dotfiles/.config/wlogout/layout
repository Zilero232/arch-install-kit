{
	"label" : "lock",
	"action" : "$HOME/bin/screen_lock.sh",
	"text" : "Lock (L)",
	"keybind" : "l"
}

{
	"label" : "logout",
	"action" : "systemctl suspend && $HOME/bin/screen_lock.sh",
	"text" : "Logout (E)",
	"keybind" : "e"
}

{
	"label" : "shutdown",
	"action" : "systemctl poweroff",
	"text" : "Shutdown (S)",
	"keybind" : "s"
}

{
	"label" : "reboot",
	"action" : "systemctl reboot",
	"text" : "Restart (R)",
	"keybind" : "r"
}