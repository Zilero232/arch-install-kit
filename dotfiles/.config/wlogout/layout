{
	"label" : "lock",
	"action" : "$HOME/bin/screen_lock.sh",
	"text" : "Lock",
	"keybind" : "l"
}

{
	"label" : "logout",
	"action" : "systemctl suspend && $HOME/bin/screen_lock.sh",
	"text" : "Logout",
	"keybind" : "e"
}

{
	"label" : "shutdown",
	"action" : "systemctl poweroff",
	"text" : "Shutdown",
	"keybind" : "s"
}

{	
	"label" : "reboot",
	"action" : "systemctl reboot",
	"text" : "Restart",
	"keybind" : "r"
}